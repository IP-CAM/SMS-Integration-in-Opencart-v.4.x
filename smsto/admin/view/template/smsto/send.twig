{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>{{ heading_title }}</h1>
            <ol class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                <li class="breadcrumb-item"><a href="{{ breadcrumb . href }}">{{ breadcrumb . text }}</a></li>
                {% endfor %}
            </ol>
        </div>
    </div>
    <div class="container-fluid">
        <iframe id='smstoiframe' src="{{ vue_url }}" frameborder="0" width="100%" height="800"></iframe>
    </div>
</div>
{{ footer }}